{
    "name": "starfolksoftware/laravel-dashboard-table-tile",
    "description": "A tile for displaying a table in spatie's laravel dashboard.",
    "keywords": [
        "starfolksoftware",
        "laravel-dashboard-table-tile"
    ],
    "homepage": "https://github.com/starfolksoftware/laravel-dashboard-table-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Faruk Nasir",
            "email": "faruk@starfolksoftware.com",
            "homepage": "https://starfolksoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "fakerphp/faker": "^1.19",
        "orchestra/testbench": "^8.0",
        "spatie/laravel-dashboard": "^3.0"
    },
    "require-dev": {
        "nunomaduro/laravel-mojito": "^0.2.10",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "StarfolkSoftware\\TableTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StarfolkSoftware\\TableTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StarfolkSoftware\\TableTile\\TableTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
