{
    "name": "starfolksoftware/laravel-dashboard-table-tile",
    "description": "A tile for displaying a table in spatie's laravel dashboard.",
    "keywords": [
        "starfolksoftware",
        "laravel-dashboard-table-tile"
    ],
    "homepage": "https://github.com/starfolksoftware/laravel-dashboard-table-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Faruk Nasir",
            "email": "faruk@starfolksoftware.com",
            "homepage": "https://starfolksoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "StarfolkSoftware\\TableTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StarfolkSoftware\\TableTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "StarfolkSoftware\\TableTile\\TableTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
